<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var user in ViewBag.Users)
    {
        <tr>
            <td>@user.first_name @user.last_name</td>
            <td>@user.email</td>
        </tr>
    }
    </tbody>
</table>